{
  "private": true,
  "name": "react-hooks-sandbox",
  "version": "1.0.0",
  "description": "Interview Tests based on React Hooks and RTL",
  "license": "CC0-1.0",
  "author": "Luca Borrione",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luca-borrione/react-hooks-sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/luca-borrione/react-hooks-sandbox/issues"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "9.39.2",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.12.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jsdom": "27.4.0",
    "lint-staged": "16.2.7",
    "prettier": "3.8.0",
    "prettier-package-json": "2.8.0",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vite": "7.3.1",
    "vitest": "4.0.17"
  },
  "keywords": [
    "hooks",
    "react",
    "rtl"
  ],
  "engines": {
    "node": "24.13.0",
    "yarn": "4.12.0"
  },
  "packageManager": "yarn@4.12.0"
}
